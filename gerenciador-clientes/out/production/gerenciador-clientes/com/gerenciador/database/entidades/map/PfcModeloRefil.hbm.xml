<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.gerenciador.database.entidades">

	<class name="PfcModeloRefil" table="pfc_modelos_refil">
	
		<id name="codModelo" column="cod_modelo_refil" type="integer" />
		
		<property name="nomeModelo" column="nome_modelo" type="string" />
		
		<property name="preco" column="preco" type="java.math.BigDecimal" />
		
		<property name="descricao" column="descricao" type="string" />
		
	</class>
	
</hibernate-mapping>